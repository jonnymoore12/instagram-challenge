<%= @user.email %>'s profile page
<br />

<% @user.posts.each do |post| %>
  <%= post.caption %>
  <br />
  <%= link_to image_tag(post.image.url(:medium)), post_path(post) %>
  <br />
  <%= pluralize post.likes.count, 'like' %>
  <%= link_to "Like this post", post_likes_path(post), method: :post %>
  <br />
<% end %>
